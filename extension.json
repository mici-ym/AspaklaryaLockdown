{
	"manifest_version": 2,
	"name": "AspaklaryaLockdown",
	"version": "1.0",
	"url": "https://www.mediawiki.org/wiki/Extension:AspaklaryaLockdown",
	"author": "[https://www.hamichlol.org.il/משתמש:מקוה מקוה]",
	"descriptionmsg": "aspaklarya_lockdown-desc",
	"license-name": "GPL-3.0-or-later",
	"type": "other",
	"requires": {
		"MediaWiki": ">= 1.40.3"
	},
	"MessagesDirs": {
		"AspaklaryaLockdown": [
			"i18n"
		]
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\AspaklaryaLockDown\\": "includes/"
	},
	"GroupPermissions": {
		"aspaklaryaEditor": {
			"aspaklarya-read-locked": true,
			"aspaklarya-read-semi-locked": true,
			"aspaklarya-edit-locked": true,
			"aspaklarya-edit-semi-locked": true,
			"aspaklarya_lockdown": true,
			"aspaklarya-lockdown-logs": true,
			"aspaklarya-lockdown-list": true,
			"aspaklarya-lock-revisions": true
		},
		"sysop": {
			"aspaklarya-read-locked": true,
			"aspaklarya-read-semi-locked": true,
			"aspaklarya-edit-locked": true,
			"aspaklarya-edit-semi-locked": true,
			"aspaklarya_lockdown": true,
			"aspaklarya-lockdown-logs": true,
			"aspaklarya-lockdown-list": true,
			"aspaklarya-lock-revisions": true
		}
	},
	"AvailableRights": [
		"aspaklarya-read-locked",
		"aspaklarya-read-semi-locked",
		"aspaklarya-edit-locked",
		"aspaklarya-edit-semi-locked",
		"aspaklarya_lockdown",
		"aspaklarya-lock-revisions",
		"aspaklarya-lockdown-logs",
		"aspaklarya-lockdown-list"
	],
	"Hooks": {
		"LoadExtensionSchemaUpdates": "schema",
		"getUserPermissionsErrors": "main",
		"BeforeParserFetchTemplateRevisionRecord": "main",
		"PageDeleteComplete": "main",
		"InfoAction": "main",
		"BeforePageDisplay": "main",
		"SkinTemplateNavigation::Universal": "main",
		"GetLinkColours": "main",
		"EditPage::showReadOnlyForm:initial": "main",
		"EditPage::showEditForm:initial": "main",
		"RandomPageQuery": "main",
		"GetPreferences": "main",
		"MediaWikiServices": "services",
		"ArticleRevisionVisibilitySet": "revision",
		"ArticleContentOnDiff": "revision",
		"DifferenceEngineNewHeader": "revision",
		"DifferenceEngineOldHeader": "revision",
		"ApiCheckCanExecute": "api",
		"APIGetAllowedParams": "api",
		"ApiQueryBaseBeforeQuery": "api",
		"APIQueryAfterExecute": "api"
	},
	"HookHandlers": {
		"schema": {
			"class": "MediaWiki\\Extension\\AspaklaryaLockDown\\Hooks\\SchemaUpdater"
		},
		"main": {
			"class": "MediaWiki\\Extension\\AspaklaryaLockDown\\Hooks\\AspaklaryaLockdown"
		},
		"api": {
			"class": "MediaWiki\\Extension\\AspaklaryaLockDown\\Hooks\\ApiHooks"
		},
		"revision": {
			"class": "MediaWiki\\Extension\\AspaklaryaLockDown\\Hooks\\RevisionHooks"
		},
		"services": {
			"class": "MediaWiki\\Extension\\AspaklaryaLockDown\\Hooks\\ServicesHook"
		}
	},
	"LogTypes": [
		"aspaklarya"
	],
	"LogNames": {
		"aspaklarya": "aspaklaryalockdownlog"
	},
	"LogHeaders": {
		"aspaklarya": "aspaklaryalockdownlogtext"
	},
	"LogActionsHandlers": {
		"aspaklarya/hide": "MediaWiki\\Extension\\AspaklaryaLockDown\\ALLogFormatter",
		"aspaklarya/unhide": "MediaWiki\\Extension\\AspaklaryaLockDown\\ALLogFormatter",
		"aspaklarya/*": "LogFormatter"
	},
	"LogRestrictions": {
		"aspaklarya": "aspaklarya-lockdown-logs"
	},
	"Actions": {
		"aspaklarya_lockdown": "MediaWiki\\Extension\\AspaklaryaLockDown\\ALockDownAction"
	},
	"APIModules": {
		"aspaklaryalockdown": "MediaWiki\\Extension\\AspaklaryaLockDown\\API\\ApiALockdown",
		"aspaklaryalockdownrevision": "MediaWiki\\Extension\\AspaklaryaLockDown\\API\\ApiALockdownRevision"
	},
	"ExtensionMessagesFiles": {
		"AspaklaryaLockdownAlias": "AspaklaryaLockdown.alias.php"
	},
	"SpecialPages": {
		"Aspaklaryalockedpage": {
			"class": "MediaWiki\\Extension\\AspaklaryaLockDown\\Special\\AspaklaryaLockedPages",
			"services": []
		},
		"Revisionlock": {
			"class": "MediaWiki\\Extension\\AspaklaryaLockDown\\Special\\ALSpecialRevisionLock",
			"services": [
				"PermissionManager"
			]
		},
		"Lockedtitles": {
			"class": "MediaWiki\\Extension\\AspaklaryaLockDown\\Special\\AspaklaryaLockedTitles",
			"services": [
				"LinkBatchFactory",
				"DBLoadBalancer",
				"TitleFormatter"
			]
		}
	},
	"GrantPermissionGroups": {
		"aspaklarya": "administration"
	},
	"GrantPermissions": {
		"aspaklarya": {
			"aspaklarya-read-locked": true,
			"aspaklarya-read-semi-locked": true,
			"aspaklarya-edit-locked": true,
			"aspaklarya-edit-semi-locked": true,
			"aspaklarya_lockdown": true,
			"aspaklarya-lock-revisions": true
		}
	},
	"config": {},
	"ResourceFileModulePaths": {
		"localBasePath": "resources",
		"remoteExtPath": "AspaklaryaLockDown/resources"
	},
	"ResourceModules": {
		"ext.aspaklaryaLockDown.styles": {
			"styles": "ext.aspaklaryaLockDown.css"
		},
		"ext.aspaklaryalockdown.nolinks": {
			"styles": "ext.aspaklaryalockdown.nolinks.css"
		},
		"ext.aspaklaryalockdown.messages": {
			"scripts": "ext.aspaklaryalockdown.messages.js",
			"messages": [
				"al-notify-read",
				"al-notify-read-semi",
				"al-notify-edit-semi",
				"al-notify-edit",
				"al-template-content-read",
				"al-template-content-edit",
				"al-template-content-edit-semi",
				"al-template-content-read-semi"
			],
			"dependencies": [
				"mediawiki.util",
				"mediawiki.jqueryMsg"
			]
		}
	},
	"DefaultUserOptions": {
		"aspaklarya-linksread": "0",
		"aspaklarya-linksedit": "0",
		"aspaklarya-linksedit-semi": "1",
		"aspaklarya-linksedit-full": "1",
		"aspaklarya-linksread-semi": "0"
	}
}